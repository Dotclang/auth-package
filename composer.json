{
    "name": "dotclang/auth-package",
    "description": "Custom authentication Laravel with Tailwind UI",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Dotclang\\AuthPackage\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Edy Ridwan Hutagalung"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Dotclang\\AuthPackage\\AuthServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dotclang/auth-package.git"
        }
    ],
    "require-dev": {
        "pestphp/pest": "4.x-dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
